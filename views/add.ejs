<% include ./partials/header %>

<div class="container">
   
   <h1><%= title %></h1>

   <form class="" action="" method="post">

      <div class="form-group">
         <label for="string">STRING</label>
         <input type="text" class="form-control" name="string2" id="string" placeholder="String">
      </div>

      <div class="form-group">
         <label for="integer">Integer</label>
         <input type="text" class="form-control" name="integer2" id="integer" placeholder="Integer">
      </div>

      <div class="form-group">
         <label for="float">Float</label>
         <input type="text" class="form-control" name="float2" id="float" placeholder="Float">
      </div>

      <div class="form-group">
         <label for="date">Date</label>
         <input type="date" class="form-control" name="date2" id="date" placeholder="MM/DD/YYY">
      </div>

      <div class="form-group">
         <label for="boolean">Boolean</label>
         <select id="boolean" class="form-control" name="boolean2">
            <option>Choose the boolean ...</option>
            <option value="true">true</option>
            <option value="false">false</option>
         </select>
      </div>

      <button type="submit" class="btn btn-primary">Save</button>
   </form>
</div>

<% include ./partials/footer %>